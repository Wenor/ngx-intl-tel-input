{
  "name": "@wenor/ngx-intl-tel-input",
  "version": "18.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wenor/ngx-intl-tel-input.git"
  },
  "exports": {
    "./build/package.json": {
      "default": "./build/package.json"
    },
    ".": {
      "types": "./build/index.d.ts",
      "esm2022": "./build/esm2022/wenor-ngx-intl-tel-input.mjs",
      "esm": "./build/esm2022/wenor-ngx-intl-tel-input.mjs",
      "default": "./build/fesm2022/wenor-ngx-intl-tel-input.mjs"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4800",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build ngx-intl-tel-input && npm run npm_pack && cp-cli ./dist/ngx-intl-tel-input ./build",
    "copy-license": "cp-cli LICENSE ./dist/ngx-intl-tel-input/LICENSE",
    "copy-readme": "cp-cli README.md ./dist/ngx-intl-tel-input/README.md",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "cd dist/ngx-intl-tel-input && npm pack",
    "package": "npm run build_lib && npm run npm_pack",
    "publish": "npm publish ./dist/ngx-intl-tel-input --access public",
    "deploy": "npm run package && npm run copy-files && npm run publish"
  },
  "dependencies": {
    "@angular/animations": "^20.3.13",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^20.3.13",
    "@angular/compiler": "^20.3.13",
    "@angular/core": "^20.3.13",
    "@angular/forms": "^20.3.13",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^20.3.13",
    "@angular/platform-browser-dynamic": "^20.3.13",
    "@angular/router": "^20.3.13",
    "core-js": "^3.22.0",
    "google-libphonenumber": "^3.2.27",
    "intl-tel-input": "^18.1.6",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1",
    "rxjs": "^7.5.5",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.11",
    "@angular/cli": "^20.3.11",
    "@angular/compiler-cli": "^20.3.13",
    "@angular/language-service": "^20.3.13",
    "@types/node": "^20.2.5",
    "ajv": "^8.11.0",
    "cp-cli": "^2.0.0",
    "ng-packagr": "^20.3.2",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "typescript": "5.9.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
